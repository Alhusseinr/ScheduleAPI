@model List<AppointmentDataAccess.appointment>

@{
    ViewBag.Title = "GetAllAppointment";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>GetAllAppointment</h2>


@foreach (var item in Model) {
<div class="row">
    <div class="col-2">
        @item.appointmentId
    </div>
    <div class="col-2">
        @item.appointmentStartDate
    </div>
    <div class="col-2">
        @item.appointmentEndDate
    </div>
    <div class="col-2">
        @item.cancelled
    </div>
    <div class="col-2">
        @item.completed
    </div>
    <div class="col-2">
        @item.confirmed
    </div>
</div>
   
   
   
    
    
    
}
